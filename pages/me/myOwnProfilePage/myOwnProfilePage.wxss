    padding-right: 30px;
}

.bottom-bar-right {
    padding-left: 30px;
}

.bottom-bar-button.active {
    color: orange;
}

.bottom-bar-mid {
    position: absolute;
    width: 54px;
    height: 54px;
    left: 50%;
    margin-left: -30px;
    top: -7px;
    border: 1px solid #bbbbbb;
    border-radius: 50%;
    background: orange;
}

.bottom-bar-mid image {
    position: absolute;
    top: 18px;
    left: 15px;
    height: 24px;
    width: 30px;
}

.bottom-bar-mid::after {
    width: 3px;
    height: 30px;
}

.bottom-bar-mid::before {
    width: 30px;
    height: 3px;
}

.bottom-bar-mid::before,.bottom-bar-mid::after {
    content: " ";
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    background-color: #fff;
}

