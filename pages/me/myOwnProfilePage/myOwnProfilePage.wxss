    height: 50px;
    color: #000;
    box-sizing: border-box;
    z-index: 100;
    display: flex;
    -webkit-box-align: center;
    transition: all .2s ease-in;
    box-shadow: 0px -1px 2px 0px lightgrey;
    transition: .5s;
}

.bottom-bar-button {
    flex: 1;
    width: 50%;
