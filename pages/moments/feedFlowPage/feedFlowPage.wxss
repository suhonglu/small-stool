    height: 24px;
    width: 30px;
}

.bottom-bar-mid::after {
    width: 3px;
    height: 30px;
}

.bottom-bar-mid::before {
    width: 30px;
    height: 3px;
}

.bottom-bar-mid::before,.bottom-bar-mid::after {
    content: " ";
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    background-color: #fff;
}

