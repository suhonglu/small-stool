page {
}

.nav-bar {
    background: transparent;
    position: initial;
}

.nav-bar-mid {
    color: #000;
}

.nav-bar-right-text {
    box-shadow: none;
}

.editor-tools {
    border: 2px dashed whitesmoke;
    position: fixed;
    width: 40px;
    height: 40px;
    top: 50px;
    left: 10px;
}

.editor-tools__delete {
    position: absolute;
    top: -3px;
    left: -3px;
}

.editor-tools__transform {
    position: absolute;
    bottom: -3px;
    right: -3px;
}

.previewWrapper {
    margin-top: 14px;
}

.previewImg {
    margin: 0 auto;
    display: block;
}

.canvasBoard {
    margin: 0 auto;
    height: 300px;
    width: 300px;
    background: white;
}

.hatCanvasBoard {
    position: absolute;
    top: 0;
    width: 300px;
    left: 50%;
    transform: translateX(-50%);
}

.optBtnGroup {
    width: 250px;
    margin: 0 auto;
}

.zoomOptBar {
    font-size: 12px;
    width: 300px;
    margin: 0 auto;
}

slider {
    margin: 14px 0;
}

.content-wrapper {
    box-sizing: border-box;
    position: relative;
    margin-top: -14px;
}

.sub-btns {
    overflow: hidden;
    font-size: 14px;
    color: #666666;
    letter-spacing: 0;
    text-align: center;
    line-height: 63px;
    padding: 6px 10px;
    margin-top: 10px;
}

.main-btns {
    padding: 4px 10px 0px;
}

.sub-btns .sub-btn {
    color: #333333;
    font-size: 15px;
    text-align: center;
    padding: 0 15px;
    background: #fff;
    border-radius: 17px;
    height: 34px;
    line-height: 34px;
    border: 1px solid #eee;
}

.sub-btn:only-child {
    float: inherit;
}

.save-btn {
    float: right;
}

.sub-btns .share-btn {
    background: #1aad19;
    color: white;
}

.userDefineImg-btn {
    float: left;
}

.avatarEditor {
    position: relative;
    margin: 0 auto;
}

.hat {
    height: 120px;
    width: 100%;
}

image {
    width: 100%;
    height: 100%;
    background: transparent;
}

.avatar {
    position: absolute;
    top: 80px;
    left: 50%;
    transform: translateX(-50%);
}

.avatar-clipbox {
    border-radius: 50%;
    height: 200px;
    width: 200px;
    overflow: hidden;
    position: relative;
}

.avatar-img {
    position: absolute;
    top: 0;
    left: 0;
}

.fn-btn {
    height: 45px;
    background: #ffee3b;
    box-shadow: 0 6px 13px 0 rgba(243,209,72,0.60);
    border-radius: 50px;
    font-size: 18px;
    line-height: 45px;
    font-weight: bold;
    color: #333333;
    letter-spacing: 0;
    text-align: center;
}

.frame-selector-wrapper {
    position: fixed;
    bottom: 0;
    width: 100%;
    box-sizing: border-box;
    overflow-x: scroll;
    overflow-y: hidden;
    text-align: center;
    white-space: nowrap;
    background: #ffffff;
    border: 1px solid #e3e3e3;
}

.frame-selector-list {
}

.frame-selector-tip {
    font-size: 14px;
    color: #666666;
    text-align: center;
    line-height: 35px;
    padding: 0 10px;
}

.frame-selector-list__item image {
    border: 1px solid #ececec;
    vertical-align: top;
    height: 50px;
    width: 50px;
    border-radius: 5px;
    overflow: hidden;
}

.frame-selector-list__item.active .item-image {
    border-color: #ffee3b;
}

.frame-selector-list__item.active image {
    border-color: #ffee3b;
}

.frame-selector-list__item.active .item-desc {
    font-weight: bolder;
    color: #444;
}

.frame-selector-list__item .item-desc {
    font-size: 12px;
    color: #666666;
    line-height: 25px;
}

.frame-selector-list__item .item-image {
    border: 3px solid white;
    border-radius: 8px;
    overflow: hidden;
    display: inline-block;
    vertical-align: top;
}

.frame-selector-list__item {
    display: inline-block;
    padding: 0 10px;
    vertical-align: top;
}

.fl {
    float: left;
}

.fr {
    float: right;
}

.frame-selector-btn {
    padding: 0 10px;
}